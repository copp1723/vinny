# OpenRouter API Configuration
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_DEFAULT_MODEL=anthropic/claude-3.5-sonnet

# Email Configuration (Alternative to email-config.json)
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=your-agent@gmail.com
EMAIL_SMTP_PASS=your-app-password
EMAIL_IMAP_HOST=imap.gmail.com
EMAIL_IMAP_PORT=993
EMAIL_AGENT_EMAIL=your-agent@gmail.com
EMAIL_RECIPIENTS=recipient1@company.com,recipient2@company.com

# Webhook Configuration (Optional)
# URL of the webhook server used to receive and retrieve real 2-FA codes
WEBHOOK_URL=http://localhost:3000
# Port the local webhook server listens on (default 3000)
WEBHOOK_PORT=3000

# VinSolutions Credentials
VINSOLUTIONS_USERNAME=your-username
VINSOLUTIONS_PASSWORD=your-password
VINSOLUTIONS_URL=https://vinsolutions.app.coxautoinc.com/vinconnect/pane-both/vinconnect-dealer-dashboard

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/agent.log

# Download Settings
DOWNLOAD_DIR=./downloads
SCREENSHOT_DIR=./screenshots

